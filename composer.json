{
    "name": "qrlogin/qrlogin",
    "type": "phpbb-extension",
    "description": "With this extension you can login to forum width qrLogin.",
    "homepage": "http://qrlogin.info",
    "version": "1.1.2",
    "time": "2017-09-17",
    "keywords": [
        "phpbb",
        "extension",
        "qrlogin"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "qrlogin",
            "homepage": "http://qrlogin.info",
            "email": "qrlogin.info@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "kairos/phpqrcode" : "~1.0"
    },
    "extra": {
        "display-name": "qrLogin",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.0,<3.3.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/qrlogin",
            "filename": "version_check"
        }
    }
}